<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.id_semaforo %>" method="POST">

          <div class="form-group">
            <input
              type="number"
              name="id_semaforo"
              value="<%= data.id_semaforo%>"
              placeholder="id_semaforo"
              class="form-control"
              autofocus
            />
          </div>

          <div class="form-group">
            <input
              type="text"
              name="name"
              value="<%= data.color%>"
              placeholder="Name"
              class="form-control"
              autofocus
            />
          </div>
          <div class="form-group">
            <input
              type="text"
              name="address"
              value="<%= data.por_zona%>"
              placeholder="Address"
              class="form-control"
            />
          </div>
          <div class="form-group">
            <input
              type="text"
              name="phone"
              value="<%= data.por_contacto%>"
              placeholder="Phone"
              class="form-control"
            />
          </div>

          <div class="form-group">
            <input
              type="text"
              name="name"
              value="<%= data.por_sintoma%>"
              placeholder="Name"
              class="form-control"
              autofocus
            />
          </div>
          <div class="form-group">
            <input
              type="text"
              name="address"
              value="<%= data.id_cuarentena%>"
              placeholder="Address"
              class="form-control"
            />
          </div>
         
         

          <button type="submit" class="btn btn-primary">save semaforoO</button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
